{
    "name": "Components-js",
    "version": "0.0.1",
    "devDependencies": {
        "gulp": "3.8.10",
        "gulp-run": "1.6.5",
        "gulp-less": "2.0.1",
        "gulp-concat": "2.4.3",
        "gulp-uglify": "1.0.2",
        "gulp-minify-css": "0.3.11",
        "gulp-jshint": "1.9.0",
        "browser-sync": "1.8.2",
        "browserify": "3.44.2",
        "vinyl-source-stream": "1.0.0",
        "vinyl-buffer": "1.0.0",
        "reactify": "0.17.1",
        "del": "1.1.1",
        "superagent": "3.5.0",
        "react": "15.4.2",
        "react-dom": "15.4.2"
    },
    "paths": {
        "less": "./assets/less/*.less",
        "js": "./app/**/*.js",
        "jsx": "./app/**/*.jsx",
        "app": "./app/app.js",
        "html": "*.html",
        "images": ["./assets/images/*.png"],
        "libs": "./libs/*.js",
        "html": "./html/*.html"
    },
    "dest": {
        "style": "style.css",
        "app": "app.js",
        "dist": "dist",
        "images": "./dist/images",
        "libs": "./dist/libs"
    }
}
